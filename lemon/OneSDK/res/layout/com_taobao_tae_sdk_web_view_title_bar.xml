<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/com_taobao_tae_sdk_web_view_title_bar"
    android:layout_width="match_parent"
    android:layout_height="45dp"
    android:background="#FFCF7C" >

    <!--android:src="@drawable/com_taobao_tae_sdk_web_view_title_bar_back"-->
    <ImageView
        android:id="@+id/com_taobao_tae_sdk_web_view_title_bar_back_button"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:paddingLeft="13dp"
        android:paddingRight="13dp"
        android:scaleType="center"
        android:src="@drawable/bar_back_selector"
        android:contentDescription="@string/com_taobao_tae_sdk_back_message" />

    <TextView
        android:id="@+id/com_taobao_tae_sdk_web_view_title_bar_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="160dp"
        android:layout_centerInParent="true"
        android:singleLine="true"
        android:textColor="#FFFFFF"
        android:textSize="18sp" />

